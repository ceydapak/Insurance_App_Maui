        </Tab>
    </FlyoutItem>
    <!--<FlyoutItem Title="Insurance Company Detail"
                Icon="insurance.png">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:Company}" Route="Company" />
        </Tab>
